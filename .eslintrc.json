{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "@react-native-community",
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "6",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": { "react/jsx-filename-extension": [0] }
}
